Class Hanoi
{
	void hanoi(int n,char from,char to,char aux)
	{
		if(n==1)
		{
			callout("printf","From %c to %c",from,to);
		}
		else
		{
			callout("hanoi",n-1,from,aux,to);
			callout("printf","Move %d from %c from to %c aux",n,from,to);
			callout("hanoi",n-1,aux,to,from);
		}
		return;
	}
	int main()
	{
		int n;
		callout("scanf","%d",&n);
		char f;
		f='A';
		char t;
		t='B';
		char a;
		a='C'
		callout("hanoi",n,f,t,a);
		return 0;
	}
}
