class Program
{
	int arr[10];
	boolean linear(int elem, int size)
	{
		int i;
		for i=0,size
		{
			if(arr[i]==elem)
			{
				return true;
			}
			i+=1;
		}
		return false;
	}
	int main()
	{
		int n;
		int m;
		int i;
		boolean f;
		n=10;
		for i=0,n
		{
			arr[i]=i;
			i+=1;
		}
		m=3;
		f=linear(m,n);
		if(f)
		{
			callout("printf","ELEMENT FOUND\n");
		}
		else
		{
			callout("printf","ELEMENT NOT FOUND\n");
		}
		return 0;
	}
}
