class Program
{
	boolean prime(int n)
	{
		int i;
		for i=2,n-1
		{
			if(n%i==0)
			{
				return false;
			}
		}
		return true;
	}
	int main()
	{
		int n;
		boolean ans;
		ans=prime(n);
		if(ans==true)
		{
			callout("printf", "NUMBER IS PRIME\n");
		}
		else
		{
			callout("printf", "NUMBER IS NOT PRIME\n");
		}
		return 0;
	}
}
