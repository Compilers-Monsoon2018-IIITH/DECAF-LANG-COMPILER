class Program
{
	int vec1[10];
	int vec2[10];
	int sum_vec[10];
	void vecadd(int size)
	{
		int i;
		for i=0,size
		{
			sum_vec[i]=vec1[i]+vec2[i];
			i+=1;
		}
		return;
	}
	int main()
	{
		int n;
		int i;
		n=10;
		for i=0,n
		{
			vec1[i]=i;
			vec2[i]=2*i;
			i+=1;
		}
		vecadd(n);
		for i=0,n
		{
			callout("printf","%d\n",sum_vec[i]);
			i+=1;
		}
		return 0;
	}
}
